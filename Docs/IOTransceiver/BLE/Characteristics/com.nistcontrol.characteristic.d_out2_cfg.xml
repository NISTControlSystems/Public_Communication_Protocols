        </Bit>
        <Bit
          index="4"
          size="3"
          name="DIGITAL_INPUT_NUMBER">
          <Enumerations>
            <Enumeration_Range
              start="0"
              end="7"
              name="DIGITAL_INPUT_NUMBER" />
          </Enumerations>
        </Bit>
        <Bit
          index="7"
          size="1"
          name="INVERT_VALUE">
          <Enumerations>
            <Enumeration
              key="0"
              value="DONT_INVERT_INPUT_VALUE"
              description="Don't invert the value of the input" />
